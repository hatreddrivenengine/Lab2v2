<Window x:Class="Lab2v2.UpdateInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab2v2"
        mc:Ignorable="d"
        Title="UpdateInfo" Height="255" Width="651">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="11,11,0,0" TextWrapping="Wrap" Text="Создано:" VerticalAlignment="Top" Height="17" Width="64"/>
        <TextBox IsReadOnly="True" Name ="NewThreats" HorizontalAlignment="Left" Height="18" Margin="78,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="30"/>
        <TextBlock HorizontalAlignment="Left" Margin="233,11,0,0" TextWrapping="Wrap" Text="Обновлено:" VerticalAlignment="Top" Height="17" Width="70"/>
        <TextBox IsReadOnly="True" Name ="UpdatedThreats" HorizontalAlignment="Left" Height="18" Margin="308,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="26"/>
        <TextBox IsReadOnly="True" x:Name ="UpdateStatus" HorizontalAlignment="Left" Height="170" Margin="21,44,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="597" TextChanged="UpdateStatus_TextChanged"/>

    </Grid>
</Window>
